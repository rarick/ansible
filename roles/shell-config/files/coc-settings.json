{
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.hintSign": "➤",
  "coc.preferences.enableLinkedEditing": true,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.formatOnSave": false,
  "coc.preferences.formatOnType": true,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "coc.source.file.ignoreHidden": false,
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "colors.enable": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.floatConfig": {
    "border": true,
    "rounded": true
  },
  "diagnostic.format": "%message [%source]",
  "diagnostic.virtualText": false,
  "go.goplsOptions": {
    "experimentalPostfixCompletions": true,
    "hints": {
      "assignVariableTypes": true,
      "compositeLiteralFields": true,
      "compositeLiteralTypes": true,
      "constantValues": true,
      "functionTypeParameters": true,
      "parameterNames": true,
      "rangeVariableTypes": true
    },
    "semanticTokens": false,
    "staticcheck": true,
    "usePlaceholders": true
  },
  "go.goplsPath": "gopls",
  "hover.floatConfig": {
    "border": true,
    "rounded": true
  },
  "inlayHint.display": false,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "languageserver": {},
  "links.highlight": true,
  "links.tooltip": true,
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-n>": "do:previewdown",
    "<C-p>": "do:previewup"
  },
  "list.normalMappings": {
    "<C-c>": "do:exit",
    "<C-n>": "do:previewdown",
    "<C-p>": "do:previewup"
  },
  "notification.statusLineProgress": true,
  "pullDiagnostic.onChange": false,
  "pullDiagnostic.onSave": true,
  "pyright.organizeimports.provider": "pyright",
  "python.analysis.diagnosticMode": "workspace",
  "python.analysis.stubPath": "~/src/python-type-stubs",
  "python.formatting.provider": "ruff",
  "python.linting.enabled": true,
  "python.linting.ruffEnabled": true,
  "semanticTokens.enable": false,
  "signature.floatConfig": {
    "border": true,
    "rounded": true
  },
  "signature.target": "echo",
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.defaultSortMethod": "none",
  "suggest.detailField": "abbr",
  "suggest.floatConfig": {
    "border": true,
    "rounded": true
  },
  "suggest.selection": "recentlyUsedByPrefix",
  "tsserver.experimental.enableProjectDiagnostics": true,
  "tsserver.implicitProjectConfig.checkJs": true,
  "tsserver.useLocalTsdk": true,
  "typescript.autoClosingTags": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.format.semicolons": "ignore",
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "literals",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "workspace.ignoredFolders": [
    "$HOME",
    "$HOME/.cargo/**",
    "$HOME/pkg/mod/**",
    "$HOMEBREW_PREFIX",
    "$HOMEBREW_PREFIX/**"
  ]
}
